{
  "name": "backend-express-template",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --testLocationInResults --setupFiles dotenv/config --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test npm run test -- --watch",
    "start": "node -r dotenv/config server.js",
    "start:watch": "nodemon -r dotenv/config server.js",
    "heroku-migrate": "heroku run npm run db:migrate",
    "heroku-reset": "heroku run npm run db:reset"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@types/jest": "^28.1.1",
    "@types/pg": "^8.6.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "sequelize": "^6.21.3"
  }
}
